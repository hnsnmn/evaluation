<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="evalSeasonEventListener" class="net.madvirus.eval.query.evalseason.EvalSeasonEventListener">
        <constructor-arg ref="evalSeasonModelRepository" />
        <constructor-arg ref="userModelRepository" />
    </bean>

    <bean class="net.madvirus.eval.query.evalseason.EvanSeasonModelInitializer">
        <constructor-arg ref="evalSeasonCluster" />
    </bean>
</beans>